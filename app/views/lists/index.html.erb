  <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <%= link_to "My lists", "", class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "New list", new_list_path, class: "nav-link" %>
      </li>
      <li class="nav-item dropdown">
        <%= image_tag "https://media-exp1.licdn.com/dms/image/C5603AQEqkUYjaeYq_A/profile-displayphoto-shrink_200_200/0/1516904422537?e=1625097600&v=beta&t=WEn9RR4E4avAmHH4pbs4syAY1HSBebzzbCi7kYcMkg0", class: "avatar-bordered", alt: "avatar-bordered" %>
